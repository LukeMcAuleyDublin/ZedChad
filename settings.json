{
  "show_edit_predictions": false,
  "assistant": {
    "default_model": {
      "provider": "copilot_chat",
      "model": "gpt-4o"
    },
    "version": "2"
  },
  "icon_theme": "Zed (Default)",
  "theme": "Kanagawa Wave",
  "base_keymap": "VSCode",
  "buffer_font_family": "JetBrainsMono Nerd Font",
  "buffer_font_size": 16,
  "ui_font_family": "JetBrainsMono Nerd Font",
  "ui_font_size": 16,
  "vim_mode": true,
  "relative_line_numbers": true,
  "cursor_blink": false,
  "scrollbar": {
    "show": "never"
  },
  "buffer_line_height": {
    "custom": 1.7
  },
  "terminal": {
    "dock": "right",
    "font_family": "JetBrainsMono Nerd Font",
    "font_size": 16,
    "line_height": {
      "custom": 1.3
    }
  },
  "languages": {
    "Ruby": {
      "language_servers": ["ruby-lsp", "!solargraph", "!rubocop", "..."]
    }
  }
}
